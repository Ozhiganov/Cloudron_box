    "test": "scripts/checkInstall.sh && npm run-script create_testdb && NODE_ENV=test ./node_modules/istanbul/lib/cli.js test $1 ./node_modules/mocha/bin/_mocha -- -R spec ./src/test ./src/routes/test ./installer/test",
    "precommit": "/bin/true",
    "prepush": "npm test",
    "postmerge": "/bin/true",
    "migrate_data": "DATABASE_URL=sqlite3:///home/yellowtent/data/cloudron.sqlite db-migrate up"
