  "scripts": {
    "create_testdir": "rm -rf $HOME/.cloudron_test; mkdir -p $HOME/.cloudron_test; cd $HOME/.cloudron_test; mkdir -p data/appdata data/appicons data/mail configs/nginx/cert configs/nginx/applications configs/collectd/collectd.conf.d configs/addons",
    "create_testdb": "NODE_ENV=test DATABASE_URL=sqlite3:///$HOME/.cloudron_test/data/cloudron.sqlite node_modules/.bin/db-migrate up",
    "migrate": "mkdir -p $HOME/.cloudron/data; DATABASE_URL=sqlite3:///$HOME/.cloudron/data/cloudron.sqlite node_modules/.bin/db-migrate up",
    "migrate_data": "DATABASE_URL=sqlite3:///home/yellowtent/data/cloudron.sqlite db-migrate up",
    "test": "scripts/checkInstall && npm run create_testdir && npm run create_testdb && NODE_ENV=test ./node_modules/istanbul/lib/cli.js test $1 ./node_modules/mocha/bin/_mocha -- -R spec ./src/test ./src/routes/test",
    "postmerge": "/bin/true",
    "precommit": "/bin/true",
    "prepush": "npm test",
    "webadmin": "node_modules/.bin/gulp"
  }
}
